-- Standard Library
def runtime(name: String) := {
  def call := function_call(name)
  def return := function_return(name)
  at(return, time(return) - time(call))
}

def at[A,B](trigger: Events[A], values: Events[B]) :=
  if time(trigger) == time(values) then values

def sum(x: Events[Int]) := {
  def s: Events[Int] := default(last(s,x) + x, 0)
  s
}

def average(x: Events[Int]) := {
  def c := count(x)
  if c != 0 then sum(x) / c
}

def count[A](x: Events[A]) := {
  def s: Events[Int] := default(last(s,x) + 1, 0)
  s
}
in line: Events[Int]
