in x: Events[Int]

def y := x + 1

def o1 := ${
  xp1 = x + 1, -- Here x refers to the member x, not the outer variable x
  x: Events[Int] = default(last(x, y) + y, 0) -- Member definitions can be recursive
}

def o2 := ${x, y} -- In this shorthand notation the members x and y are assigned the values of
                  -- the outer variables of the same name

out o1.xp1
out o1.x
out o2.x
out o2.y