in instruction: Events<String>
in function: Events<String>
in line: Events<Int>
in column: Events<Int>

def last(s) := last(s,s)

def eventCount(e) := {
  def ec := default(last(ec, e) + 1, 0)
  ec
}

def function_call(name) :=
  if function == name && last(function) != name && last(instruction) == "call" then ()

def f_count := eventCount(function_call("f"))

out f_count